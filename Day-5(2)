import java.util.*;
class harsed
{
public static void main(String args[])
{
int N,s=0,n,r=0,re;
Scanner sc=new Scanner(System.in);
System.out.println("Enter n value");
N=sc.nextInt();
n=N;
while(N!=0){
rem=N%10;
s=s+r;
N=N/10;
}

if(N%s==0)
{
System.out.println(n+" Is harshed number");
}
else
{
System.out.println(n+" Is Not harshed number");
}
}
}
